    <!-- The string used as the preference file name. -->
    <string name="com_ovrhere_morseflash_PREFERENCE_FILE_KEY" >
        com_ovrhere_morseflash_PREFERENCE_FILE_KEY
    </string>
    
    <!-- Preference keys:
	BE WARY OF CHANGING KEY VALUES HERE.  -->
